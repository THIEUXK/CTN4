
@{
    ViewData["Title"] = "Themdiachi";
}



@model List<CTN4_Data.Models.DB_CTN4.DiaChiNhanHang>

@{
    ViewData["Title"] = "Themdiachi";
}
<section class="banner-area organic-breadcrumb">
    <div class="container">
        <div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
            <div class="col-first">
                <h1>Đăng Ký</h1>
                <nav class="d-flex align-items-center">
                    <a href="">Home<span class="lnr lnr-arrow-right"></span></a>
                    <a href="">Thêm Địa Chỉ</a>
                </nav>
            </div>
        </div>
    </div>
</section>
<hr />
<div class="row" >
    <div class="col-md-6">
    
        <form  method="post">
        <div class="form-group">
            <label for="Tên">Tên:</label>

                <select name="name" class="form-control">
                @foreach (var item in Model)
                {
                        <option value="@item.name">@item.name</option>
                }
            </select>
        </div>
            <div class="form-group">
                <label for="code">Phonecode:</label>
                <select name="phone_code" class="form-control">
                    @foreach (var item in Model)
                    {
                        <option value="@item.phone_code">@item.phone_code</option>
                    }
                </select>
            </div>
        <div class="form-group">
            <label for="code">Mã:</label>
                <select name="code" class="form-control">
                @foreach (var item in Model)
                {
                        <option  value="@item.code">@item.code</option>

                }
            </select>
        </div>

        <div class="form-group">
            <label for="division_type">Loại:</label>

                <select name="division_type" class="form-control">
                @foreach (var item in Model)
                {
                        <option  value="@item.division_type">@item.division_type</option>

                }
            </select>
        </div>

        <div class="form-group">
            <label for="codename">Mã tên:</label>

                <select name="codename" class="form-control">
                @foreach (var item in Model)
                {
                        <option  value="@item.codename">

                            @item.codename</option>
                }
            </select>
            <div class="form-group">
                <label for="diaChi">Địa chỉ:</label>
                <input type="text" class="form-control" id="diaChi" name="DiaChi" placeholder="Nhập địa chỉ của bạn" required>
            </div>
                <button asp-action="themdiachis" type="submit" class="btn btn-primary">Submit</button>
        </div>
        </form>
    </div>
</div>
        
<div>
    <a asp-action="Index">Back to List</a>
</div>

